{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017 
  },
  "extends": [
    "standard",
    "prettier",
    "prettier/standard",
    "plugin:cypress/recommended"
  ],
  "plugins": [
    "prettier",
    "standard",
    "cypress"
  ],
  "env": {
    "node": true,
    "es6": true,
    "jest": true,
    "cypress/globals": true
  },
  "rules": {
    "prettier/prettier": 2
  }
}
